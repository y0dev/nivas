                    .company-address(class="lg:w-1/2 px-6")
                        h2.title-font.font-semibold.text-gray-900.tracking-widest.text-xs Address
                        p.mt-1 Dallas, Texas
                    .company-email-phone(class="lg:w-1/2 px-6 mt-4 lg:mt-0")
                        h2.title-font.font-semibold.text-gray-900.tracking-widest.text-xs Email
                        a.text-primary.leading-relaxed(href="#") contact@urbaninsightinc.com 
                        h2.title-font.font-semibold.text-gray-900.tracking-widest.text-xs.mt-4 Phone
                        p.leading-relaxed 123-456-7890

            .contact-container(class="group p-6 mt-4 sm:mt-0 sm:p-8 md:w-1/2 w-full rounded-3xl bg-white border border-gray-100 dark:shadow-none dark:border-gray-700 dark:bg-gray-800 bg-opacity-50 shadow-2xl shadow-gray-600/10")
