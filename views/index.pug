                        input#name.form__input(class="relative bg-white m-0 -mr-0.5 block w-full min-w-0 flex-auto rounded border border-solid border-neutral-300 bg-transparent bg-clip-padding px-3 py-[0.25rem] text-base font-normal leading-[1.6] text-gray-700 outline-none transition duration-200 ease-in-out focus:z-[3] focus:border-primary focus:text-neutral-700 focus:shadow-[inset_0_0_0_1px_rgb(59,113,202)] focus:outline-none dark:border-neutral-600 dark:text-neutral-200 dark:placeholder:text-gray-200 dark:focus:border-primary" type='text', placeholder='Full Name', required)
                    .form__group.relative.mb-4
                        label.form__label(for='email' class="mt-6 mb-8 text-gray-600 dark:text-gray-300") Email address
                        input#email.form__input(class="relative bg-white m-0 -mr-0.5 block w-full min-w-0 flex-auto rounded border border-solid border-neutral-300 bg-transparent bg-clip-padding px-3 py-[0.25rem] text-base font-normal leading-[1.6] text-gray-700 outline-none transition duration-200 ease-in-out focus:z-[3] focus:border-primary focus:text-neutral-700 focus:shadow-[inset_0_0_0_1px_rgb(59,113,202)] focus:outline-none dark:border-neutral-600 dark:text-neutral-200 dark:placeholder:text-gray-200 dark:focus:border-primary" type='email', placeholder='you@example.com', required)
                    .form__group.relative.mb-4
                        label.form__label(for='phone' class="mt-6 mb-8 text-gray-600 dark:text-gray-300") Phone Number
                        input#phone.form__input(class="relative bg-white m-0 -mr-0.5 block w-full min-w-0 flex-auto rounded border border-solid border-neutral-300 bg-transparent bg-clip-padding px-3 py-[0.25rem] text-base font-normal leading-[1.6] text-gray-700 outline-none transition duration-200 ease-in-out focus:z-[3] focus:border-primary focus:text-neutral-700 focus:shadow-[inset_0_0_0_1px_rgb(59,113,202)] focus:outline-none dark:border-neutral-600 dark:text-neutral-200 dark:placeholder:text-gray-200 dark:focus:border-primary" type='phone', placeholder='555-555-5555' required)
                    .form__group.relative.mb-4
                        label.form__label(for='message' class="mt-6 mb-8 text-gray-600 dark:text-gray-300") Subject
                        textarea#message.form__input(class="relative resize-none bg-white m-0 -mr-0.5 block w-full min-w-0 flex-auto rounded border border-solid border-neutral-300 bg-transparent h-32 bg-clip-padding px-3 py-[0.25rem] text-base font-normal leading-[1.6] text-gray-700 outline-none transition duration-200 ease-in-out focus:z-[3] focus:border-primary focus:text-neutral-700 focus:shadow-[inset_0_0_0_1px_rgb(59,113,202)] focus:outline-none dark:border-neutral-600 dark:text-neutral-200 dark:placeholder:text-gray-200 dark:focus:border-primary" placeholder='Message') 
                    .form__group
                        button(class="relative flex h-11 w-full items-center justify-center px-6 before:absolute before:inset-0 before:rounded-full before:bg-primary before:transition before:duration-300 hover:before:scale-105 active:duration-75 active:before:scale-95") 
